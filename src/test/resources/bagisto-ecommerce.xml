<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite1" verbose="1" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="actions.reportConfigs.AllureTestListener"/>
    </listeners>


    <test name="Run Test Bagisto Ecommerce on chrome">
        <!-- Define parameters -->
        <parameter name="environment" value="local"/>
        <parameter name="browser" value="chrome"/>
        <parameter name="urlAdmin" value="https://demo.bagisto.com/bagisto-common/admin/login"/>
        <parameter name="urlUser" value="https://demo.bagisto.com/bagisto-common/"/>

        <classes>
            <class name="e2efollow.AdminSettingAccount"></class>
        </classes>
    </test>

</suite>
