<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="NopCommerce - Web Automation UI">

    <test name="Run on LambdaTest">
        <parameter name="environment" value="sauce_lab"/>
        <parameter name="osName" value="Windows 11"/>
        <parameter name="osVersion" value="11"/>
        <parameter name="browserName" value="chrome"/>
        <parameter name="browserVersion" value="latest"/>
        <parameter name="urlAdmin" value="https://demo.bagisto.com/bagisto-common/admin/login"/>
        <parameter name="urlUser" value="https://demo.bagisto.com/bagisto-common/"/>

        <classes>
            <class name="e2efollow.AdminSettingAccount"/>
        </classes>
    </test>

</suite>
